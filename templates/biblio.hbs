{{#if citation_key}}

<dt data-key="{{citation_key}}">
    {{citation_key}}
</dt>
<dd>
    <a href="{{url}}" target="_blank">{{/if}}<cite>{{title}}</cite>{{#if url}}</a>
    {{#if authors}}{{authors}}{{/if}}
</dd>


<div class="bib_div">
<details data-key="{{citation_key}}" class=ref>
{{!-- citation --}}
<summary class=citation>
    <a id="{{citation_key}}">[{{citation_key}}]</a> - {{#if authors}}{{authors}} - {{/if}}{{#if title}}{{#if
    url}}<a href="{{url}}" target="_blank">{{/if}}<cite>{{title}}</cite>{{#if url}}</a>{{/if}}.{{/if}} - {{#if
    pub_year}}{{pub_year}}.{{/if}}
</summary>

{{!-- summary/abstract --}}
{{#if summary}}
<section class=abstract>
    <h1>Abstract...</h1>
    <div>{{summary}}</div>
</section>
{{/if}}

</details>
<br />

</div>

{{/if}}
